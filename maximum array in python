def find_maximum(arr):
    return max(arr) if arr else None 
    
arr = [-775, -426, 540, 768, -937, -252, -523, 421, -477, -546, 591, -641, 593, 512, 
       26, 34, 884, 687, 142, 989, -42, 468, 48, -514, 204, 864, -350, 985, -679, -9, 
       -730, 847, 295, 163, 812, -26, -400, -750, 194, -814, 358, 367, 331, -79, 231, 
       -354, -399, 333, 892, 937]

print("The maximum number is:", find_maximum(arr))
